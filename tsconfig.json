{
  "compilerOptions": {
    "alwaysStrict": true,
    "allowJs": true,
    "lib": ["ESNext"],
    "noEmit": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node"
  },
  "typeAcquisition": {
    "enable": true
  }
}

